<selector xmlns:android="http://schemas.android.com/apk/res/android"
	android:dither="true">
	<item android:state_window_focused="false" android:drawable="@drawable/circular_btn_normal" />
	<item android:state_pressed="true" android:drawable="@drawable/circular_btn_pressed" />
	<item android:state_focused="true" android:drawable="@drawable/circular_btn_pressed" />
	<item android:state_enabled="false" android:drawable="@drawable/circular_btn_disabled" />
	<item android:drawable="@drawable/circular_btn_normal" />
</selector>